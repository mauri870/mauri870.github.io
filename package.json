{
  "name": "mauri870.github.io",
  "version": "1.0.0",
  "description": "My personal website",
  "author": "Mauri de Souza Nunes <mauri870@gmail.com>",
  "private": true,
  "scripts": {
    "postinstall": "bower i",
    "dev": "node build/dev-server.js",
    "blog": "(cd blog && hugo server -D)",
    "start": "node build/dev-server.js",
    "build": "node build/build.js && (cd blog && hugo)",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "clean": "rm -r dist || true",
    "prod": "yarn run test && yarn run clean && yarn run build && git add dist && git commit -m \"Update site for production\" && git subtree push --prefix dist origin master"
  },
